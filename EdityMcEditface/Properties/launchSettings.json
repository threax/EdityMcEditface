{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:49628/",
      "sslPort": 0
    }
  },
  "profiles": {
    "EdityMcEditface": {
      "commandName": "Project",
      "commandLineArgs": "--workingDir=\"../Manual\"",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "UpdateClient": {
      "commandName": "Project",
      "commandLineArgs": "tools \"clientgen $(ProjectDir)node_modules/editymceditface.client/EditorCore/EdityHypermediaClient.ts\"",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Update Config Schema": {
      "commandName": "Project",
      "commandLineArgs": "tools updateConfigSchema",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Update Edity Schema": {
      "commandName": "Project",
      "commandLineArgs": "tools updateEditySchema",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Clone Repo": {
      "commandName": "Project",
      "commandLineArgs": "--workingDir=\"../CloneTest\" tools \"clone https://github.com/threax/EdityMcEditface.git\"",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}