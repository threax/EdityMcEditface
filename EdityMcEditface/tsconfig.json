{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "obj/tscout/lib",
    "module": "amd",
    "baseUrl": ".",
    "paths": {
      "hr.*": [
        "node_modules/HtmlRapier/src/*",
        "node_modules/htmlrapier.json-editor/*"
      ],
      "hr.widgets.*": [
        "node_modules/HtmlRapier.Widgets/src/*"
      ],
      "hr.bootstrap.*": [
        "node_modules/HtmlRapier.Bootstrap/lib/*",
        "node_modules/HtmlRapier.Bootstrap/*"
      ],
      "hr.treemenu.*": [
        "node_modules/HtmlRapier.TreeMenu/src/*"
      ],
      "edity.editorcore.*": [
        "Client/EditorCore/*"
      ],
      "edity.widgets.*": [
        "Client/Widgets/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "Client/node_modules",
    "wwwroot",
    "Client/SwaggerGen/*"
  ],
  "compileOnSave": true
}
