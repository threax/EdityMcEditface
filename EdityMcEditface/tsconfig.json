{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "outDir": "obj/tscout/lib",
    "outFile": "wwwroot/lib/tsbin.js",
    "module": "amd",
    "baseUrl": ".",
    "noEmitHelpers": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "paths": {
      "edity.editorcore.*": [
        "Editor\\*",
        "node_modules\\editymceditface.client\\EditorCore\\*"
      ],
      "hr.bootstrap.*": [
        "node_modules\\htmlrapier.bootstrap\\src\\*"
      ],
      "hr.halcyon.*": [
        "node_modules\\htmlrapier.halcyon\\src\\*"
      ],
      "jdorn.*": [
        "node_modules\\htmlrapier.json-editor\\*"
      ],
      "hr.sidebar.*": [
        "node_modules\\htmlrapier.sidebar\\src\\*"
      ],
      "hr.treemenu.*": [
        "node_modules\\htmlrapier.treemenu\\src\\*"
      ],
      "hr.widgets.*": [
        "node_modules\\htmlrapier.widgets\\src\\*"
      ],
      "hr.*": [
        "node_modules\\htmlrapier\\src\\*"
      ]
    }
  },
  "compileOnSave": true,
  "include": [
    "Editor\\**\\*.ts",
    "node_modules\\editymceditface.client\\EditorCore\\**\\*.ts",
    "node_modules\\editymceditface.client\\Views\\**\\*.ts",
    "node_modules\\htmlrapier.bootstrap\\src\\**\\*.ts",
    "node_modules\\htmlrapier.halcyon\\src\\**\\*.ts",
    "node_modules\\htmlrapier.sidebar\\src\\**\\*.ts",
    "node_modules\\htmlrapier.treemenu\\src\\**\\*.ts",
    "node_modules\\htmlrapier.widgets\\src\\**\\*.ts",
    "node_modules\\htmlrapier\\src\\**\\*.ts"
  ],
  "files": [
    "node_modules\\jsns\\jsns.ts",
    "node_modules\\htmlrapier.json-editor\\bin\\json-editor.js",
    "node_modules\\jsns\\tslib.jsns.js"
  ],
  "exclude": [
    "node_modules\\threax-npm-tk\\**\\*.js"
  ]
}