{
  "$schema": "../../../SchemaGenerator/Schema/EdityProject.json",
  "contentMap": {
    "jquery": {
      "js": [ { "file": "/lib/jquery/dist/jquery.js" } ]
    },
    "bootstrap": {
      "css": [ "/lib/bootstrap/dist/css/bootstrap.css", "/lib/bootstrap/dist/css/bootstrap-theme.css" ],
      "js": [ { "file": "/lib/bootstrap/dist/js/bootstrap.js" } ],
      "dependencies": [ "jquery" ]
    },
    "pen": {
      "css": [ "/lib/pen/src/pen.css" ],
      "js": [ { "file": "/lib/pen/src/pen.js" } ]
    },
    "edityLoading": {
      "css": [ "/edity/css/loading.min.css" ]
    },
    "htmlrest": {
      "js": [
        { "file": "/lib/htmlrest/src/polyfill.js" },
        { "file": "/lib/htmlrest/src/output.js" },
        { "file": "/lib/htmlrest/src/rest.js" },
        { "file": "/lib/htmlrest/src/storage.js" },
        { "file": "/lib/htmlrest/src/table.js" },
        { "file": "/lib/htmlrest/src/form.js" },
        { "file": "/lib/htmlrest/src/components.js" }
      ],
      "dependencies": [ "jquery" ]
    },
    "json-editor": {
      "js": [
        { "file": "/lib/json-editor/dist/jsoneditor.js" }
      ]
    }
  },
  "vars": {
    "uploadUrl": "edity/upload"
  },
  "additionalContent": [ "lib/bootstrap/dist/fonts" ],
  "compilers": [
    {
      "type": "Html",
      "template": "default.html",
      "extension": ".html"
    }
  ]
}