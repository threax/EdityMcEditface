{
  "$schema": "../../../SchemaGenerator/Schema/EdityProject.json",
  "contentMap": {
    "transitions": {
      "css": [ "/edity/css/fade.min.css" ]
    },
    "bootstrap": {
      "css": [ "/lib/bootstrap/dist/css/bootstrap.css", "/lib/bootstrap/dist/css/bootstrap-theme.css" ],
      "js": [ { "file": "/lib/bootstrap.native/dist/bootstrap-native.min.js" } ],
      "dependencies": [ "transitions" ]
    },
    "codemirror": {
      "js": [ { "file": "/lib/codemirror/lib/codemirror.js" } ],
      "css": ["/lib/codemirror/lib/codemirror.css"]
    },
    "ckeditor": {
      "js": [ { "file": "/lib/ckeditor/ckeditor.js" } ]
    },
    "edityLoading": {
      "css": [ "/edity/css/loading.min.css" ]
    },
    "htmlrest": {
      "js": [
        { "file": "/lib/htmlrest/htmlrest.js" }
      ],
      "hasMin": true
    },
    "TreeMenu": {
      "js": [
        {
          "file": "/lib/edity/widgets/TreeMenu.js",
          "async": true
        }
      ]
    },
    "TreeMenuEditor": {
      "css": [ "/lib/edity/widgets/TreeMenuEditor.css" ]
    }
  },
  "vars": {
    "uploadUrl": "/edity/upload",
    "pageSettingsUrl": "/edity/page/settings",
    "savePageUrl": "/edity/Page",
    "deletePageUrl": "/edity/Page",
    "mediaBrowseDir": "/",
    "pageTemplateUrl": "/edity/templates"
  },
  "additionalContent": [ "lib/bootstrap/dist/fonts" ],
  "compilers": [
    {
      "type": "Html",
      "template": "default.html",
      "extension": ".html"
    }
  ],
  "editComponents": [ "editComponents/commit.html", "editComponents/settings.html", "editComponents/compile.html", "editComponents/source.html", "editComponents/media.html" ]
}