{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "PageDefinition",
  "additionalProperties": false,
  "properties": {
    "vars": {
      "type": [
        "null",
        "object"
      ],
      "additionalProperties": {
        "type": "string"
      },
      "description": "The variables for this page."
    },
    "linkedContent": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      },
      "description": "The content linked to this page."
    },
    "hidden": {
      "type": "boolean",
      "description": "True if the page should be hidden and not included in output."
    }
  }
}